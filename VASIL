# Main workflow of VASIL
report: "report/workflow.rst"


rule get_lineages_frequencies:
    input:
        cases=config["cases"],
        covsonar=config["covsonardata"],
        date_start=config["date_start"],
    output:
        result="results/data/Daily_Lineages_Freq.csv",
    shell:
        "python {workflow.basedir}/scripts/SpikeGroups_frequencies/Lineages_freqs.py {input.cases} {input.covsonar} {input.date_start} {output.result}"
